{
  "name": "teambook",
  "description": "An app that allows you to find info about your colleagues",
  "author": "Simon Nonnis (https://github.com/SimoNonnis)",
  "version": "0.0.1",
  "license": "MIT",
  "main": "src/main.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot",
    "clean": "rm -rf ./dist/*",
    "prebuild": "mkdir -p ./dist && npm run clean && cp favicon.ico ./dist",
    "build": "webpack -p --config webpack.config.prod.js",
    "postbuild": "http-server ./dist -p 8081 -d false --cors -o",
    "stats": "webpack --profile --json > stats.json",
    "test": "jest"
  },
  "keywords": [
    "react",
    "reactjs",
    "webpack",
    "css-modules",
    "babel",
    "postcss",
    "postcss-cssnext",
    "web-font-loader",
    "hot-module-replacement",
    "eslint",
    "eslint-loader",
    "stylelint",
    "html-webpack-plugin",
    "http-server",
    "jest"
  ],
  "homepage": "https://github.com/SimoNonnis/teambook#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimoNonnis/teambook"
  },
  "bugs": {
    "url": "https://github.com/SimoNonnis/teambook/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-es2015-destructuring": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.9.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "http-server": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^3.0.0",
    "jest": "^16.0.2",
    "normalize.css": "^5.0.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-easy-import": "^1.0.1",
    "postcss-loader": "^1.1.0",
    "postcss-reporter": "^1.4.1",
    "react-addons-test-utils": "^15.3.2",
    "react-css-modules": "^3.7.10",
    "style-loader": "^0.13.1",
    "stylelint-webpack-plugin": "^0.4.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-validator": "^2.2.9"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css)$": "identity-obj-proxy"
    }
  }
}
